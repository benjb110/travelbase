<div class="page-header">

  <div class="row">
   <div class="col-md-8 col-md-offset-2">
     <div class="panel panel-default">
       <div class="panel-heading">
         <h3 class="panel-title">
           All Users
         </h3>
       </div>

       <% @users.each do |user| %>

       <ul class="list-group">
        <li class="list-group-item">
          <a href="/users/<%= user.id %>"
          </a>
          <%=  user.username  %>
          </a>
        </li>
           <% end %>
